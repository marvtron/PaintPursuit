{
  "name": "paintpursuit",
  "version": "1.0.0",
  "description": "An application with an \"instagram-esque\" UI intended to make a fun 'geocache' type community for finding and sharing murals and street art with friends. Points accumulate for a friendly competition element.",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvtron/PaintPursuit.git"
  },
  "keywords": [],
  "author": "Marvin Tryon Jr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marvtron/PaintPursuit/issues"
  },
  "homepage": "https://github.com/marvtron/PaintPursuit#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-iframe": "^1.8.0",
    "validator": "^10.11.0"
  }
}
